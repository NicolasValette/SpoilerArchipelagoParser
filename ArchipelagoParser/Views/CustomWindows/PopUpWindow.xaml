<Window x:Class="NoNiDev.ArchipelagoParser.Views.CustomWindows.PopUpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NoNiDev.ArchipelagoParser.Views"
        mc:Ignorable="d"
        Title="PopUpWindow" Height="200" Width="300">
    <Grid >
        <TextBox x:Name="UserInput" HorizontalAlignment="Center" Margin="5,55,5,5" Width="200" Text="Nom du joueur" TextWrapping="Wrap"  VerticalAlignment="Top" />
        <Button Content="Close" Click="Button_Click" ClickMode="Release" HorizontalAlignment="Center" Height="25"  VerticalAlignment="Bottom" Width="75" Margin="5,5,5,5"/>

    </Grid>
</Window>
